<?xml version="1.0" encoding="utf-8"?>
<resources>
    <dimen name="toolbarHeight">56dp</dimen>
    <dimen name="toolbarTabHeight">48dp</dimen>

    <style name="toolbarStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/toolbarHeight</item>
        <item name="android:background">@color/fn_primary</item>
    </style>

    <style name="toolbarTabListStyle">
        <item name="android:background">@color/fn_primary</item>
    </style>

    <style name="toolbarTabListContainerStyle">
        <item name="android:layout_height">@dimen/toolbarTabHeight</item>
    </style>

    <style name="toolbarConfirmStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/toolbarHeight</item>
        <item name="android:background">#FF0000FF</item>
        <item name="android:textSize">12sp</item>
    </style>
</resources>