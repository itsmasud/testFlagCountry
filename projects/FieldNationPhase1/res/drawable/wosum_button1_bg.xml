<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle" >
            <corners
                android:bottomLeftRadius="2dp"
                android:bottomRightRadius="2dp"
                android:topLeftRadius="2dp"
                android:topRightRadius="2dp" />

            <solid android:color="@color/wosumButton1Bevel" >
            </solid>
        </shape>
    </item>
    <item
        android:bottom="2px"
        android:left="2px">
        <shape android:shape="rectangle" >
            <corners
                android:bottomLeftRadius="2dp"
                android:bottomRightRadius="2dp"
                android:topLeftRadius="2dp"
                android:topRightRadius="2dp" />

            <gradient
                android:angle="-90"
                android:endColor="@color/wosumButton1GradientBot"
                android:startColor="@color/wosumButton1GradientTop" />
        </shape>
    </item>

</layer-list>