<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle" >
            <corners
                android:bottomLeftRadius="@dimen/wosum_corner_radius"
                android:topLeftRadius="@dimen/wosum_corner_radius" />

            <solid android:color="@color/wosumStatus6Bevel" >
            </solid>
        </shape>
    </item>
    <item android:bottom="@dimen/wosum_bevel_size">
        <shape android:shape="rectangle" >
            <corners
                android:bottomLeftRadius="@dimen/wosum_corner_radius"
                android:topLeftRadius="@dimen/wosum_corner_radius" />

            <solid android:color="@color/wosumStatus6" />
        </shape>
    </item>

</layer-list>