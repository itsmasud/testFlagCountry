<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="wosum">
        <item name="android:background">@color/wosumBackground</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="wosum_inside"></style>

    <style name="data_container_viewgroup">
        <item name="android:background">@color/wosum_data_container_background</item>
        <item name="android:paddingLeft">10px</item>
        <item name="android:paddingRight">10px</item>
        <item name="android:paddingTop">10px</item>
        <item name="android:paddingBottom">10px</item>
    </style>

    <style name="wosum_status_bar">
        <item name="android:background">@drawable/wosum_status_1_no_highlight</item>
    </style>

    <style name="wosum_button">
        <item name="android:textColor">#FFFFFFFF</item>
        <item name="android:foreground">#FFFFFFFF</item>
        <item name="android:background">@drawable/wosum_button_background</item>
    </style>

    <style name="wosum_label">
        <item name="android:textColor">#FFc1c1a9</item>
        <item name="android:paddingRight">5dp</item>
    </style>

</resources>